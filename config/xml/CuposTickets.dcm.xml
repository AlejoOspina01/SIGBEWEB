<!-- config/xml/Roles.dcm.xml -->
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
	https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

	<entity name="CuposTickets" table="cupostickets">
		<id name="consecutivo_cupostickets" type="integer">
			<generator strategy="AUTO" />
		</id>

		<field name="fechaasignacion" type="date" />
		<field name="cupostotal" type="integer" />
		<field name="cuposdisponibles" type="integer" />
	</entity>
</doctrine-mapping>